{
    "name": "notion-clone/php-backend",
    "description": "PHP backend for Notion clone",
    "type": "project",
    "require": {
        "php": "^8.0",
        "slim/slim": "^4.9",
        "slim/psr7": "^1.5",
        "php-di/php-di": "^6.3",
        "firebase/php-jwt": "^6.0",
        "vlucas/phpdotenv": "^5.4",
        "monolog/monolog": "^2.3",
        "tuupola/slim-jwt-auth": "^3.5",
        "doctrine/orm": "^2.10",
        "symfony/cache": "^5.3",
        "ramsey/uuid": "^4.2",
        "guzzlehttp/guzzle": "^7.4",
        "intervention/image": "^2.7",
        "cboden/ratchet": "^0.4.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "start": "php -S localhost:8000 -t public",
        "websocket": "php bin/websocket-server.php",
        "test": "phpunit tests"
    }
}
